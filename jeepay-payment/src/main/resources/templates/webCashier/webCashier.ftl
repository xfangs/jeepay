<script src="https://cdn.staticfile.org/layui/2.4.3/layui.min.js"></script>
<script>

var payOrderToken = "${orderId}";

// 全部支付方式
var allWaycode = ["ALI_BAR","ALI_JSAPI","ALI_PC","WX_LITE","WX_BAR","QR_CASHIER","ALI_WAP","WX_JSAPI","ALI_APP","WX_H5","WX_APP","WX_NATIVE","ALI_QR","ALI_GLOBAL_WAP"];

// 倒计时： s
var downSecond = 7155;

layui.use(['jquery', 'element', 'layer', 'util'], function(){

    let $ = layui.$;
    var element = layui.element;
    var layer = layui.layer;
    var util = layui.util;

    let initFunc = function(){
        // 判断具体的支付方式
        $(".paycashier-type").each(function(){
            if(allWaycode.indexOf($(this).attr('wayCode')) >= 0){
                $(this).removeClass('layui-hide');
            }
        });

        // 支付方式分类是否显示
        $(".paytype-li").each(function(index){
            let liThis = $(this);
            $('.paytype-li-content').eq(index).find('.paycashier-type').each(function(){
                if(!$(this).hasClass('layui-hide')){
                    liThis.removeClass('layui-hide');
                }
            })
        })

        // 点击第一个分类
        $(".paytype-li").each(function(index){
            if(!$(this).hasClass('layui-hide')){
                $(this).click();
                return false; // 不在循环
            }
        })
    }


    // 初始化
    initFunc();

    // 请求后盾代码封装公共函数
    let $reqApi = function (url, method, data){
        return new Promise((resolve, reject) => {
            $.ajax({
                url : url,
                type: method,
                data: data,
                success: function(data){

                    if(data && data.code == 0){
                        return resolve(data.data)
                    }else{
                        layer.alert(data.msg)
                        return reject(data.msg);
                    }

                },error: function(){
                    return reject('请求接口异常');
                }
            });
        })
    }



    // 切换支付方式
    $(".paycashier-type").click(function(){
        $(".paycashier-type").removeClass('paycashier-type-this');
        $(this).addClass('paycashier-type-this');
    })

    // 点击 确认支付
    $('.confirmBtn').click(function(){

        // 支付方式
        let wayCode = $('.paycashier-type-this').attr('wayCode');
        if(!wayCode){
            return layer.msg('请选择支付方式')
        }

        let payDataType = null;
        // 根据不同的支付方式， 设置不同的支付信息
        if(wayCode == 'QR_CASHIER' || wayCode == 'ALI_QR' || wayCode == 'WX_NATIVE' || wayCode == 'UP_QR'){
            payDataType = 'codeImgUrl'
        }

        $reqApi('${baseURL}/api/webCashier/convertPayway/' + payOrderToken, 'post', {wayCode: wayCode, payDataType: payDataType}).then((bizData)=>{
              console.info(bizData);

               location.href = bizData.payUrl;

<#--            if(bizData.payDataType == 'codeImgUrl'){-->
<#--                queryOrderTask(); //轮询查单-->
<#--                layer.open({-->
<#--                    title: "扫码支付", type: 1, closeBtn: 0, area: ['300px', '370px'],-->
<#--                    content: '<div style="display: flex;flex-direction: column;align-items: center;padding-top: 40px;">' +-->
<#--                                '<img src="'+bizData.payData+'" />' +-->
<#--                             '<div style="padding-top: 20px;display: flex;flex-direction: row;align-items: center;justify-content: center;"></div></div>'-->
<#--                });-->

<#--            }else if(bizData.payDataType == 'payurl'){-->


<#--                location.href = bizData.payData;-->
<#--            }-->

        });
    })

    // 定时轮询查询订单
    let queryOrderTask = () => {
        $reqApi('${baseURL}/api/webCashier/orderState/' + payOrderToken, 'post').then((bizData)=>{
            if(bizData.state == 2){   // 支付成功
                if(bizData.returnUrl){  // 跳转到对应的页面（如果有）
                    location.href = bizData.returnUrl;
                }else{
                    layer.msg('支付成功', {icon: 1, time: 0});
                }
            }else{
                setTimeout(queryOrderTask, 5000); // 继续查询
            }
        })
    }

    // 聚合支付： web收银台 转换支付
    $(".qrCashierBtn").click(function(){

        $reqApi('${baseURL}/api/webCashier/convertPayway/' + payOrderToken, 'post', {wayCode: "QR_CASHIER", payDataType: "codeImgUrl"}).then((bizData)=>{
                queryOrderTask(); //轮询查单
                layer.open({
                    title: "扫码支付", type: 1, closeBtn: 0, area: ['300px', '370px'],
                    content: '<div style="display: flex;flex-direction: column;align-items: center;padding-top: 40px;">' +
                        '<img src="'+bizData+'" />' +
                        '<div style="padding-top: 20px;display: flex;flex-direction: row;align-items: center;justify-content: center;">' +
                        '</div></div>',
                });
        });
    });

    // 定时更新倒计时内容
    let downMinTask = () => {

        let hour = parseInt(downSecond/3600);
        let min = parseInt( (downSecond - hour * 3600 ) / 60);
        let second = (downSecond - hour * 3600 ) % 60;

        // 包含小时
        if(hour > 0){
            $('#downMinSpan').text( ( hour < 10 ? ('0' + hour) : hour ) + ":" + ( min < 10 ? ('0' + min) : min ) + ":" + ( second < 10 ? ('0' + second) : second ) )
        }else{
            $('#downMinSpan').text( ( min < 10 ? ('0' + min) : min ) + ":" +  ( second < 10 ? ('0' + second) : second ) )
        }

        downSecond--;

        if(downSecond < 0){
            return ;
        }

        setTimeout(downMinTask, 1000);
    }
    downMinTask();

});




















</script>
